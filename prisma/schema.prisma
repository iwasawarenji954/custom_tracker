generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Habit {
  id          Int     @id @default(autoincrement())
  name        String
  iscompleted Boolean @default(false) // ここを小文字に変更
  score       Int     @default(1)
  createdAt   DateTime @default(now())
}


